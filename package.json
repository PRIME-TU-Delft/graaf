		"@sveltejs/kit": "^2.20.4",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@types/node": "^22.14.0",
		"@vitest/ui": "3.0.6",
		"autoprefixer": "^10.4.21",
		"bits-ui": "1.3.13",
		"clsx": "^2.1.1",
