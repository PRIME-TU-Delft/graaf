		"formsnap": "^2.0.0",
		"globals": "^15.15.0",
		"lucide-svelte": "^0.469.0",
		"mode-watcher": "^0.5.1",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"prisma": "^6.5.0",
		"svelte": "^5.25.8",
		"svelte-check": "^4.1.5",
		"svelte-sonner": "^0.3.28",
		"sveltekit-superforms": "^2.24.1",
