		"@prisma/client": "^6.5.0",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.16",
		"@tanstack/table-core": "^8.21.2",
		"@thisux/sveltednd": "^0.0.19",
		"@types/d3": "^7.4.3",
		"d3": "^7.9.0",
		"postgres": "^3.4.5",
		"tsx": "^4.19.3"
